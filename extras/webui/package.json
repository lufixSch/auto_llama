{
	"name": "webui",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"webui-install": "vite build && cp package.json build && npm install dotenv && cd build && npm install --omit=dev",
		"webui": "node -r dotenv/config build",
		"auto_llama_sdk": "npx openapi-typescript-codegen --input http://127.0.0.1:8000/openapi.json --output src/lib/auto_llama/"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@types/eslint": "^8.56.0",
		"@types/markdown-it": "^13.0.7",
		"@types/node": "^20.11.25",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.18",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-motion": "^0.12.2",
		"svelte-preprocess": "^5.1.3",
		"tailwind-merge": "^2.2.1",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"openapi-typescript-codegen": "^0.29.0"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.6.7",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.5",
		"markdown-it": "^14.0.0",
		"markdown-it-katex": "^2.0.3",
		"openai": "^4.28.4",
		"svelte-gestures": "^4.0.0",
		"turndown": "^7.1.3"
	}
}
